<%- include('../layouts/header') %>
<%- include('../layouts/utils/navbar') %>
<div class="container">
    <h1 class="my-3 text-center">Add Mahasiswa Data</h1>

    <div class="col-md-4 offset-4">
        <div class="text-center">
            <form class="form-group" action="/mahasiswa/" method="post">
                <div class="form-floating mb-2">
                    <input type="text" name="name" id="name" class="form-control" placeholder="Name" autofocus required>
                    <label for="name">Name</label>
                </div>
                <div class="form-floating mb-2">
                    <input type="email" name="email" id="email" class="form-control" placeholder="Email" required>
                    <label for="email">Email</label>
                </div>
                <button class="btn btn-outline-success form-control" type="submit">Save</button>
            </form>
        </div>
    </div>

</div>
<%- include('../layouts/footer') %>